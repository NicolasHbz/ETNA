struct	s_list
{
  void		*data;
  struct s_list	*next;
}		t_list;

t_list *add_list(t_list **list, void data)
{
  t_list = *node;

  node = malloc(sizeof(t_list));
  if (node == NULL)
    {
      return(NULL);
    }
  node->next = *list;
  node->data = data;
  *list = node;
}

