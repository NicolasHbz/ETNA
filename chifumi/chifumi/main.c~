#include "fonction.h"
#include <stdlib.h>
#include <stdio.h>

int	main (int argc, char **argv)
{
  if (!error_manage(argc, argv))
    {
      my_putstr("\nMauvais arguments.");
      my_putstr("\nMODELE: --version \"classique ou expert\"");
      my_putstr(" --duree \"un nombre impair\"\n\n");
      return (0);      
    }
  menu(argv);
  return(0);
}
  
