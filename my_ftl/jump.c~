#include <stdlib.h>
#include "ftl.h"
#include "function.h"

void		jump(t_ship *ship)
{
  if (ship->navigation_tools->sector <= 10)
      ship->navigation_tools->sector = ship->navigation_tools->sector + 1;
} 
