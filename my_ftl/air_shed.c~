#include <stdlib.h>
#include "ftl.h"
#include "function.h"

t_ship		*create_ship ()
{
  t_ship	*create;
  
  my_putstr("construction du vaisseau en cours...\n");
  create = malloc(sizeof(*create));
  if (create == NULL)
    {
      my_putstr("le vaisseau n'a pas pu etre construit par manque de materiaux \n");
    }
  my_putstr("amelioration du vaisseau termine!\n");
  return create;
}
		  

  
  
