void	my_putchar(char c);

void	my_aff_up_params(int argc, char **argv)
{
  int	i;
  int	j;

  i = 0;
  while (i <= argc)
    {
      j = 0;
      while (argv[i][j] != '\0')
	{
	  if (argv[i][j] >= 'a'&& argv[i][j] <= 'z')
	    {
	      argv[i][j] = argv[i][j] - 32;
	    }
	  my_putchar(argv[i][j]);    
	  ++j;
	}
      my_putchar('\n');
      ++i;
    }
}
