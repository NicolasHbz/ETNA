int             main(void)
{
  char          *str = "Hello world!";
  char          *new_str;

  new_str = my_strdup(str);
  printf("valeur de new_str : %s\n", new_str);
  printf("adresse contenue dans str : %p\n", str);
  printf("adresse contenue dans new_str : %p\n", new_str);
  return (0);
}
