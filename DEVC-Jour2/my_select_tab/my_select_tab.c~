void my_putchar(char c);

char  my_select_tab(char tab[4][14], int i, int j)
{
  int x;
  int y;
  char caractere;

  x = 0;
  y = 0;

  while (x < i)
    {
      x = x + 1;
        while (y < j)
	  {
	    y = y + 1;
	  }
    }
  caractere = tab[x][y]; 
  
  return caractere;
}
