#include "liblist.h"
#include <stdlib.h>
#include <stdio.h>

t_list		*delete_element_from_doubly_list(t_list *list)
{
  if (list == NULL)
    {
      return 0;
    }
  else if (list->next == NULL || list->prev == NULL)
    {
      if (list->next == NULL)
	{
	  list->prev->next = NULL;
	}
      else
	{
	  list->next->prev = NULL;
	}
    }
  else
    {
      list->prev->next = list->next;
      list->next->prev = list->prev;
    }
  free(list);
}

  
